    <% for (cat of catInfo) { %>
      <img class="my-2 mx-auto" src=" <%= cat.pic %> " alt="car1">
      <h1 class="my-2">
        Name: <%= cat.name %>
      </h1>
      <h3 class="my-2">Favorite food: <%= cat.favfood %></h3>
      <h3 class="my-2">Age: <%= cat.age %></h3>
      <h3 class="my-2">Sleeping Spots:</h3>
      <ul>
        <% for (spot of cat.sleepspot) { %>
        <li><%= spot %></li>
        <% } %>
      </ul>
      <hr>
     <% } %>
  


</body>
